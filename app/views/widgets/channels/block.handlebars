<script id="channel-template-small" type="text/x-handlebars-template">
	<header>
		<div class="triangles">
			<span class="triangle triangle-green"></span>
			<span class="triangle triangle-red"></span>
		</div>
		<span class="title">{{title}} - <span>Topline Stats</span></span>
		<ul class="options">
			<li data-type="WoW">WoW</li>
			<li data-type="MoM">MoM</li>
			<li data-type="QoQ">QoQ</li>
			<li data-type="YoY">YoY</li>
		</ul>
		<a href="widgets/channels/small/export/{{title}}?category={{category}}" class="export" target="blank">Export</a>
	</header>
	<div class="rows">
		<div class="section">
			<div class="title">Orgs</div>
			<div class="last_month">
				<span class="title">Last {{timeframe}}</span>
				<span class="number">{{data.1.orgs}}</span>
			</div>
			<div class="last_month">
				<span class="title">This {{timeframe}}</span>
				<span class="number">{{data.0.orgs}}</span>
			</div>
		</div>
		<div class="section">
			<div class="title">Advs</div>
			<div class="last_month">
				<span class="title">Last {{timeframe}}</span>
				<span class="number">{{data.1.advs}}</span>
			</div>
			<div class="this_month">
				<span class="title">This {{timeframe}}</span>
				<span class="number">{{data.0.advs}}</span>
			</div>
		</div>
		<div class="section">
			<div class="title">Billed Spend</div>
			<div class="last_month">
				<span class="title">Last {{timeframe}}</span>
				<span class="number">{{data.1.billed_spend}}</span>
			</div>
			<div class="this_month">
				<span class="title">This {{timeframe}}</span>
				<span class="number">{{data.0.billed_spend}}</span>
			</div>
		</div>
		<div class="section">
			<div class="title">SOV</div>
			<div class="last_month">
				<span class="title">Last {{timeframe}}</span>
				<span class="number">{{data.1.sov}}</span>
			</div>
			<div class="this_month">
				<span class="title">This {{timeframe}}</span>
				<span class="number">{{data.0.sov}}</span>
			</div>
		</div>
	</div>
	<div class="bottom-bar-left"></div>
	<div class="bottom-bar-right"></div>
</script>