<script id="qb-front-item" type="text/x-handlebars-template">
{{#each items}}
	<li class="{{status}}" data-id="{{id}}">
		<span class="item-name"><span class="icon"></span><span class="name-inner">{{bucket_name}}</span> <span class="progress">{{progress}}</span></span>
		<span class="status">{{status}}</span>
		<span class="user"><span></span></span>
		<div class="details">
			<div class="line"></div>
			<div class="box-wrapper">
				<div class="box">
					<span class="info-name"><span>Name</span> {{bucket_name}}</span>
					<span class="info-by"><span>Status</span> {{status}}</span>
					<span class="comment"><span>Comment</span> {{comment.result}}</span>
					{{#if comment.missing_hours}}
						<span class="about"><span>Missing Hours</span></span>
						<ul>
							{{#each comment.missing_hours}}
								<li>{{this}}</li>
							{{/each}}
						</ul>
					{{/if}}
					{{#if comment.late_hours}}
						<span class="about"><span>Late Hours</span></span>
						<ul>
							{{#each comment.late_hours}}
								<li>{{this}}</li>
							{{/each}}
						</ul>
					{{/if}}
				</div>
			</div>
		</div>
	</li>
{{/each}}
</script>

<script id="pqt-back-url" type="text/x-handlebars-template">
	<div class="url">
		<input type="text" placeholder="Add URL" name="urls[]" />
		<a href="#" class="add"><span></span></a>
	</div>
</script>