<script id="pqt-front-template" type="text/x-handlebars-template">
	<div class="pqt">
		{{#if permission}}
			<a href="#" class="create">Create...</a>
		{{/if}}
		<img class="logo" src="_img/{{logo}}" />
		<div class="date-picker">
			<span class="arrow-left">&#60;</span>
			<input type="text" value="" />
			<span class="arrow-right">&#62;</span>
		</div>
		<ul class="header">
			<li class="time"><span></span></li>
			<li class="user"><span></span></li>
		</ul>
		<ul class="content">
			
		</ul>
	</div>
</script>

<script id="pqt-front-item" type="text/x-handlebars-template">
{{#each items}}
	<li class="{{status}}" data-id="{{id}}">
		<span class="item-name"><span class="icon"></span><span class="name-inner">{{name}}</span> <span class="progress">{{progress}}</span></span>
		<span class="time" data-time="{{time_started}}" data-finished="{{time_finished}}"></span>
		<span class="user">{{initials}}</span>
		<div class="details">
			<div class="line"></div>
			<div class="box-wrapper">
				<div class="box">
					<span class="info-name">{{name}}</span>
					<span class="info-by"><span>Run By</span>{{first_name}} {{last_name}}</span>
					<span class="info-time"><span>Time Started</span>{{time_started}}</span>
					<span class="info-time"><span>Time Finished</span>{{time_finished}}</span>
				</div>
			</div>
		</div>
	</li>
{{/each}}
</script>

